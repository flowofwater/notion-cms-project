{
	"mcpServers": {
		"playwright": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"@playwright/mcp@latest"
			],
			"env": {}
		},
		"context7": {
			"type": "stdio",
			"url": "https://mcp.context7.com/mcp"
		},
		"sequential-thinking": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@modelcontextprotocol/server-sequential-thinking"
			],
			"env": {}
		},
		"shadcn": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"shadcn@latest",
				"mcp"
			]
		},
		"shrimp-task-manager": {
			"type": "stdio",
			"command": "node",
			"args": [
				"/Users/sunwoo.jung/workspace/mcp-shrimp-task-manager/dist/index.js"
			],
			"env": {
				"DATA_DIR": "/Users/sunwoo.jung/workspace/notion-cms-project/shrimp_data",
				"TEMPLATES_USE": "en",
				"ENABLE_GUI": "true"
			}
		}
	}
}